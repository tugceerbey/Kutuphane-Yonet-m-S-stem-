@using KütüphaneSistemi.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "ÜyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Üye:@ViewBag.u1</h2>
<br />
<table class="table table-danger">
    <tr>
        <th>Kitap Adı</th>
        <th>Alış Tarihi </th>
        <th>İade Tarihi</th>
    </tr>
    @foreach (var x in Model)
    {
    <tr>
        <td>@x.TBLKITAP.AD</td>
        <td>@Convert.ToDateTime(x.ALISTARIH).ToString("dd/MM/yyyy")</td>
        <td>@Convert.ToDateTime(x.UYEGETIRTARIH).ToString("dd/MM/yyyy")</td>
    </tr>
    }


</table>
<a class="btn btn-primary" href="/Üye/Index/">Geri</a>

